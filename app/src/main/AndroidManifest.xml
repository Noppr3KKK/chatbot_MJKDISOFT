<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/iconapk"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconapk"
        android:supportsRtl="true"
        android:theme="@style/Theme.Chatbot"
        tools:targetApi="31">
        <activity
            android:name=".MainMenu"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mjkdisoft.chatbot.NormalGenre" />
        <activity android:name="com.mjkdisoft.chatbot.RomanceGenre" />
        <activity android:name="com.mjkdisoft.chatbot.ComedyGenre" />
        <activity android:name="com.mjkdisoft.chatbot.TechnologyGenre" />
    </application>

</manifest>